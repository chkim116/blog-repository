{
	"compilerOptions": {
		"strict": true,
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"removeComments": false,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"baseUrl": "./",
		"outDir": "./dist"
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"dist"
	]
}